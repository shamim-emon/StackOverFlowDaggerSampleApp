<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emon.stackoverflowdaggersampleapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StackOverFlowDaggerSampleApp">
        <activity
            android:name=".view.screen.QuestionDetailsActivity"
            android:label="@string/title_activity_question_details"
            android:theme="@style/Theme.StackOverFlowDaggerSampleApp.NoActionBar"></activity>
        <activity android:name=".view.screen.QuestionListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>